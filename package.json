{
  "name": "vite-plugin-unocss-watcher",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@8.9.2",
  "description": "A Vite plugin to reload UnoCSS config when files changed",
  "author": "ocavue <ocavue@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/ocavue",
  "homepage": "https://github.com/ocavue/vite-plugin-unocss-watcher#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ocavue/vite-plugin-unocss-watcher.git"
  },
  "bugs": "https://github.com/ocavue/vite-plugin-unocss-watcher/issues",
  "keywords": [
    "unocss",
    "vite",
    "vite-plugin"
  ],
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "fix": "eslint --fix . && prettier --write .",
    "prepublishOnly": "nr build",
    "start": "esno src/index.ts",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.8",
    "@ocavue/eslint-config": "^1.3.1",
    "@types/node": "^18.18.6",
    "@unocss/core": "^0.57.1",
    "eslint": "^8.52.0",
    "esno": "^0.17.0",
    "prettier": "^3.0.3",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  },
  "renovate": {
    "extends": [
      "github>ocavue/config-renovate"
    ]
  }
}
